mixin listItem(parcel)
    tr
        td #{parcel.id}
        td
            ul
                li #{parcel.order}
                li #{parcel.receiver}
                li #{parcel.orderDate}
        td #{parcel.destination}
        td
            img(src=parcel.itemInHaigerIcon)
        td
            ul
                li #{parcel.sentFromHaigerWith}
                li #{parcel.sentFromHaigerDate}
        td
            img(src=parcel.arrivedAtDestinationIcon)
        td #{parcel.comment}
        td
            a(href="/parcel/"+parcel.itemProcessedAction+"/"+parcel.id)
                img(src=parcel.itemProcessedIcon)
        td
            a(href="/parcel/form/" + parcel.id) edit